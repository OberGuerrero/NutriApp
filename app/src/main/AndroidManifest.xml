<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-feature android:name="android.hardware.camera"
        android:required="true" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo_nutriapp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_nutriapp"
        android:supportsRtl="true"
        android:theme="@style/Theme.NutriApp"
        tools:targetApi="31">
        <meta-data
            android:name="androidx.camera.camera2.CameraXConfig.Provider"
            android:value="androidx.camera.camera2.CameraXConfig" />

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Registro">

        </activity>

    </application>

</manifest>